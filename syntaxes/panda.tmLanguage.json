{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "panda language syntax",
	"scopeName": "source.pd",
	"patterns": [
		{
			"include": "#preprocessor"
		},
		{
			"include": "#comment"
		},
		{
			"include": "#metadata"
		},
		{
			"include": "#namespace"
		},
		{
			"include": "#import"
		},
		{
			"include": "#declarations"
		},
		{
			"include": "#expression"
		}
	],
	"repository": {
		"preprocessor": {
			"name": "meta.preprocessor.pd",
			"begin": "^\\s*(\\#)\\s*",
			"beginCaptures": {
				"1": {
					"name": "punctuation.separator.hash.pd"
				}
			},
			"end": "(?<=$)",
			"patterns": [
				{
					"begin": "\\b(?:(if)|(elif))\\b",
					"beginCaptures": {
						"1": {
							"name": "keyword.preprocessor.if.pd"
						},
						"2": {
							"name": "keyword.preprocessor.elif.pd"
						}
					},
					"end": "(?=$)",
					"patterns": [
						{
							"include": "#preprocessor_expression"
						}
					]
				},
				{
					"match": "\\b(?:(else)|(end))\\b",
					"captures": {
						"1": {
							"name": "keyword.preprocessor.else.pd"
						},
						"2": {
							"name": "keyword.preprocessor.end.pd"
						}
					}
				}
			]
		},
		"comment": {
			"patterns": [
				{
					"name": "comment.block.pd",
					"begin": "(\\/\\*)",
					"beginCaptures": {
						"1": {
							"name": "punctuation.definition.comment.pd"
						}
					},
					"end": "(\\*\\/)",
					"endCaptures": {
						"1": {
							"name": "punctuation.definition.comment.pd"
						}
					}
				},
				{
					"name": "comment.line.double-slash.pd",
					"begin": "(\\/\\/)",
					"beginCaptures": {
						"1": {
							"name": "punctuation.definition.comment.pd"
						}
					},
					"end": "(?:\\n|$)"
				}
			]
		},
		"metadata": {
			"begin": "(@[_[:alpha:]][_[:alnum:]]*)",
			"beginCaptures": {
				"1": {
					"name": "entity.name.tag.pd"
				}
			},
			"end": "(?:\\n|$)",
			"patterns": [
				{
					"begin": "\\(",
					"beginCaptures": {
						"0": {
							"name": "punctuation.parenthesis.begin.pd"
						}
					},
					"end": "\\)",
					"endCaptures": {
						"0": {
							"name": "punctuation.parenthesis.close.pd"
						}
					},
					"patterns": [
						{
							"match": "([_[:alpha:]][_[:alnum:]]*)\\s*(=)",
							"captures": {
								"1": {
									"name": "entity.name.type.pd"
								},
								"2": {
									"name": "keyword.operator.assignment.pd"
								}
							}
						},
						{
							"include": "#literal"
						},
						{
							"include": "#punctuation_comma"
						}
					]
				}
			]
		},
		"namespace": {
			"begin": "\\b(namespace)\\b\\s+",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.namespace.pd"
				}
			},
			"end": "(?=;)",
			"patterns": [
				{
					"name": "entity.name.type.namespace.pd",
					"match": "[_[:alpha:]][_[:alnum:]]*"
				},
				{
					"include": "#punctuation_accessor"
				}
			]
		},
		"import": {
			"patterns": [
				{
					"begin": "\\b(import)\\b\\s+([_[:alpha:]][_[:alnum:]]*)\\s*=",
					"beginCaptures": {
						"1": {
							"name": "keyword.other.using.pd"
						},
						"2": {
							"name": "entity.name.type.alias.pd"
						}
					},
					"end": "(?=;)",
					"patterns": [
						{
							"name": "entity.name.type.namespace.pd",
							"match": "[_[:alpha:]][_[:alnum:]]*"
						}
					]
				},
				{
					"begin": "\\b(import)\\b\\s*",
					"beginCaptures": {
						"1": {
							"name": "keyword.other.using.pd"
						}
					},
					"end": "(?=;)",
					"patterns": [
						{
							"name": "entity.name.type.namespace.pd",
							"match": "[_[:alpha:]][_[:alnum:]]*"
						}
					]
				}
			]
		},
		"declarations": {
			"patterns": [
				{
					"include": "#storage_modifier"
				},
				{
					"include": "#enum"
				},
				{
					"include": "#property"
				},
				{
					"include": "#method"
				},
				{
					"include": "#interface"
				},
				{
					"include": "#class"
				}
			]
		},
		"storage_modifier": {
			"name": "storage.modifier.pd",
			"match": "\\b(public|static|async)\\b"
		},
		"enum": {
			"begin": "\\b(enum)\\b\\s+([_[:alpha:]][_[:alnum:]]*)",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.enum.pd"
				},
				"2": {
					"name": "entity.name.type.enum.pd"
				}
			},
			"end": "(?<=\\})",
			"patterns": [
				{
					"begin": "\\{",
					"beginCaptures": {
						"0": {
							"name": "punctuation.curlybrace.open.pd"
						}
					},
					"end": "\\}",
					"endCaptures": {
						"0": {
							"name": "punctuation.curlybrace.close.pd"
						}
					},
					"patterns": [
						{
							"include": "#preprocessor"
						},
						{
							"include": "#comment"
						},
						{
							"include": "#punctuation_comma"
						},
						{
							"begin": "[_[:alpha:]][_[:alnum:]]*",
							"beginCaptures": {
								"0": {
									"name": "entity.name.variable.enum_member.pd"
								}
							},
							"end": "(?=(,|\\}))",
							"patterns": [
								{
									"include": "#variable_initializer"
								}
							]
						}
					]
				}
			]
		},
		"property": {
			"begin": "\\b(const|var)\\b\\s+([_[:alpha:]][_[:alnum:]]*)",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.property.pd"
				},
				"2": {
					"name": "entity.name.variable.pd"
				}
			},
			"end": "(?=;)",
			"patterns": [
				{
					"include": "#type"
				},
				{
					"include": "#variable_initializer"
				}
			]
		},
		"method": {
			"begin": "(\\binline\\b\\s+)?(\\bfunction\\b)\\s+([_~[:alpha:]][_[:alnum:]]*)",
			"beginCaptures": {
				"1": {
					"name": "keyword.other.inline.pd"
				},
				"2": {
					"name": "keyword.other.function.pd"
				},
				"3": {
					"name": "entity.name.function.pd"
				}
			},
			"end": "(?<=\\})|(?=;)",
			"patterns": [
				{
					"include": "#type_parameter_list"
				},
				{
					"include": "#parenthesized_parameter_list"
				},
				{
					"include": "#type"
				},
				{
					"include": "#block"
				}
			]
		},
		"method_declaration": {
			"begin": "([_[:alpha:]][_[:alnum:]]*)",
			"beginCaptures": {
				"1": {
					"name": "entity.name.function.pd"
				}
			},
			"end": "(?<=\\})|(?=;)",
			"patterns": [
				{
					"include": "#type_parameter_list"
				},
				{
					"include": "#parenthesized_parameter_list"
				},
				{
					"include": "#type"
				},
				{
					"include": "#block"
				}
			]
		},
		"interface": {
			"begin": "(?=\\binterface\\b)",
			"end": "(?<=\\})",
			"patterns": [
				{
					"begin": "\\b(interface)\\b\\s+([_[:alpha:]][_[:alnum:]]*)",
					"beginCaptures": {
						"1": {
							"name": "keyword.other.interface.pd"
						},
						"2": {
							"name": "entity.name.type.interface.pd"
						}
					},
					"end": "(?=\\{)",
					"patterns": [
						{
							"include": "#type_parameter_list"
						},
						{
							"include": "#base_types"
						}
					]
				},
				{
					"begin": "\\{",
					"beginCaptures": {
						"0": {
							"name": "punctuation.curlybrace.open.pd"
						}
					},
					"end": "\\}",
					"endCaptures": {
						"0": {
							"name": "punctuation.curlybrace.close.pd"
						}
					},
					"patterns": [
						{
							"include": "#preprocessor"
						},
						{
							"include": "#comment"
						},
						{
							"include": "#metadata"
						},
						{
							"include": "#method_declaration"
						},
						{
							"include": "#punctuation_semicolon"
						}
					]
				}
			]
		},
		"class": {
			"begin": "(?=\\bclass\\b)",
			"end": "(?<=\\})",
			"patterns": [
				{
					"begin": "\\b(class)\\b\\s+([_[:alpha:]][_[:alnum:]]*)\\s*",
					"beginCaptures": {
						"1": {
							"name": "keyword.other.class.pd"
						},
						"2": {
							"name": "entity.name.type.class.pd"
						}
					},
					"end": "(?=\\{)",
					"patterns": [
						{
							"include": "#comment"
						},
						{
							"include": "#type_parameter_list"
						},
						{
							"include": "#base_types"
						}
					]
				},
				{
					"begin": "\\{",
					"beginCaptures": {
						"0": {
							"name": "punctuation.curlybrace.open.pd"
						}
					},
					"end": "\\}",
					"endCaptures": {
						"0": {
							"name": "punctuation.curlybrace.close.pd"
						}
					},
					"patterns": [
						{
							"include": "#preprocessor"
						},
						{
							"include": "#comment"
						},
						{
							"include": "#metadata"
						},
						{
							"include": "#storage_modifier"
						},
						{
							"include": "#property"
						},
						{
							"include": "#method"
						},
						{
							"include": "#punctuation_semicolon"
						}
					]
				},
				{
					"include": "#preprocessor"
				},
				{
					"include": "#comment"
				}
			]
		},
		"base_types": {
			"begin": ":",
			"beginCaptures": {
				"0": {
					"name": "punctuation.separator.colon.pd"
				}
			},
			"end": "(?=\\{)",
			"patterns": [
				{
					"include": "#type"
				},
				{
					"include": "#punctuation_comma"
				}
			]
		},
		"parenthesized_parameter_list": {
			"begin": "(\\()",
			"beginCaptures": {
				"0": {
					"name": "punctuation.parenthesis.open.pd"
				}
			},
			"end": "(\\))",
			"endCaptures": {
				"0": {
					"name": "punctuation.parenthesis.close.pd"
				}
			},
			"patterns": [
				{
					"include": "#parameter"
				},
				{
					"include": "#punctuation_comma"
				},
				{
					"include": "#variable_initializer"
				}
			]
		},
		"type_parameter_list": {
			"begin": "\\<",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.typeparameters.begin.pd"
				}
			},
			"end": "\\>",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.typeparameters.end.pd"
				}
			},
			"patterns": [
				{
					"match": "([_[:alpha:]][_[:alnum:]]*)\\b",
					"captures": {
						"1": {
							"name": "entity.name.type.type-parameter.pd"
						}
					}
				},
				{
					"begin": "\\:\\s*",
					"patterns": [
						{
							"include": "#type"
						}
					]
				},
				{
					"include": "#punctuation_comma"
				}
			]
		},
		"variable_initializer": {
			"begin": "(?<!=|!)(=)(?!=|>)",
			"beginCaptures": {
				"1": {
					"name": "keyword.operator.assignment.pd"
				}
			},
			"end": "(?=[,\\)\\];])",
			"patterns": [
				{
					"include": "#expression"
				}
			]
		},
		"type": {
			"name": "meta.type.pd",
			"patterns": [
				{
					"include": "#type_builtin"
				},
				{
					"include": "#type_name"
				},
				{
					"include": "#type_arguments"
				}
			]
		},
		"parameter": {
			"begin": "([_[:alpha:]][_[:alnum:]]*)",
			"beginCaptures": {
				"1": {
					"name": "entity.name.variable.parameter.pd"
				}
			},
			"end": "(?=,)|(?=\\))",
			"patterns": [
				{
					"include": "#type"
				},
				{
					"include": "#variable_initializer"
				}
			]
		},
		"block": {
			"begin": "\\{",
			"beginCaptures": {
				"0": {
					"name": "punctuation.curlybrace.open.pd"
				}
			},
			"end": "\\}",
			"endCaptures": {
				"0": {
					"name": "punctuation.curlybrace.close.pd"
				}
			},
			"patterns": [
				{
					"include": "#statement"
				}
			]
		},
		"statement": {
			"patterns": [
				{
					"include": "#preprocessor"
				},
				{
					"include": "#comment"
				},
				{
					"include": "#for_statement"
				},
				{
					"include": "#if_statement"
				},
				{
					"include": "#else_part"
				},
				{
					"include": "#switch_statement"
				},
				{
					"include": "#return_statement"
				},
				{
					"include": "#break_or_continue_statement"
				},
				{
					"include": "#raw_statement"
				},
				{
					"include": "#throw_statement"
				},
				{
					"include": "#yield_statement"
				},
				{
					"include": "#await_statement"
				},
				{
					"include": "#try_statement"
				},
				{
					"include": "#local_declaration"
				},
				{
					"include": "#punctuation_semicolon"
				},
				{
					"include": "#block"
				},
				{
					"include": "#expression"
				}
			]
		},
		"for_statement": {
			"begin": "(?<!\\.)\\b(for)\\b\\s*(?=\\()",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.loop.for.pd"
				}
			},
			"end": "(?<=\\})|(?=;)",
			"patterns": [
				{
					"begin": "\\(",
					"beginCaptures": {
						"0": {
							"name": "punctuation.parenthesis.open.pd"
						}
					},
					"end": "\\)",
					"endCaptures": {
						"0": {
							"name": "punctuation.parenthesis.close.pd"
						}
					},
					"patterns": [
						{
							"include": "#local_declaration"
						},
						{
							"include": "#expression"
						},
						{
							"include": "#punctuation_semicolon"
						},
						{
							"include": "#punctuation_colon"
						}
					]
				},
				{
					"include": "#statement"
				}
			]
		},
		"if_statement": {
			"begin": "(?<!\\.)\\b(if)\\b\\s*(?=\\()",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.conditional.if.pd"
				}
			},
			"end": "(?<=\\})|(?=;)",
			"patterns": [
				{
					"begin": "\\(",
					"beginCaptures": {
						"0": {
							"name": "punctuation.parenthesis.open.pd"
						}
					},
					"end": "\\)",
					"endCaptures": {
						"0": {
							"name": "punctuation.parenthesis.close.pd"
						}
					},
					"patterns": [
						{
							"include": "#expression"
						}
					]
				},
				{
					"include": "#statement"
				}
			]
		},
		"else_part": {
			"begin": "(?<!\\.)\\b(else)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.conditional.else.pd"
				}
			},
			"end": "(?<=\\})|(?=;)",
			"patterns": [
				{
					"include": "#statement"
				}
			]
		},
		"switch_statement": {
			"begin": "(?<!\\.)\\b(switch)\\b\\s*(?=\\()",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.switch.pd"
				}
			},
			"end": "(?<=\\})",
			"patterns": [
				{
					"begin": "\\(",
					"beginCaptures": {
						"0": {
							"name": "punctuation.parenthesis.open.pd"
						}
					},
					"end": "\\)",
					"endCaptures": {
						"0": {
							"name": "punctuation.parenthesis.close.pd"
						}
					},
					"patterns": [
						{
							"include": "#expression"
						}
					]
				},
				{
					"begin": "\\{",
					"beginCaptures": {
						"0": {
							"name": "punctuation.curlybrace.open.pd"
						}
					},
					"end": "\\}",
					"endCaptures": {
						"0": {
							"name": "punctuation.curlybrace.close.pd"
						}
					},
					"patterns": [
						{
							"patterns": [
								{
									"begin": "(?<!\\.)\\b(case)\\b\\s+",
									"beginCaptures": {
										"1": {
											"name": "keyword.control.case.pd"
										}
									},
									"end": ":",
									"endCaptures": {
										"0": {
											"name": "punctuation.separator.colon.pd"
										}
									},
									"patterns": [
										{
											"include": "#expression"
										}
									]
								},
								{
									"match": "(?<!\\.)\\b(default)\\b\\s*(:)",
									"captures": {
										"1": {
											"name": "keyword.control.default.pd"
										},
										"2": {
											"name": "punctuation.separator.colon.pd"
										}
									}
								}
							]
						},
						{
							"include": "#statement"
						}
					]
				}
			]
		},
		"return_statement": {
			"begin": "(?<!\\.)\\b(return)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.flow.return.pd"
				}
			},
			"end": "(?=;)",
			"patterns": [
				{
					"include": "#expression"
				}
			]
		},
		"break_or_continue_statement": {
			"match": "(?<!\\.)\\b(?:(break)|(continue))\\b",
			"captures": {
				"1": {
					"name": "keyword.control.flow.break.pd"
				},
				"2": {
					"name": "keyword.control.flow.continue.pd"
				}
			}
		},
		"raw_statement": {
			"match": "(@)",
			"captures": {
				"1": {
					"name": "entity.name.tag.pd"
				}
			}
		},
		"throw_statement": {
			"begin": "(?<!\\.)\\b(throw)\\b",
			"beginCaptures": {
				"1": {
					"name": "keyword.control.flow.throw.pd"
				}
			},
			"end": "(?=;)",
			"patterns": [
				{
					"include": "#expression"
				}
			]
		},
		"yield_statement": {
			"patterns": [
				{
					"begin": "(?<!\\.)\\b(yield)\\b\\s*\\b(return)\\b",
					"beginCaptures": {
						"1": {
							"name": "keyword.control.flow.yield.pd"
						},
						"2": {
							"name": "keyword.control.flow.return.pd"
						}
					},
					"end": "(?=;)",
					"patterns": [
						{
							"include": "#expression"
						}
					]
				},
				{
					"match": "(?<!\\.)\\b(yield)\\b\\s*\\b(break)\\b",
					"captures": {
						"1": {
							"name": "keyword.control.flow.yield.pd"
						},
						"2": {
							"name": "keyword.control.flow.break.pd"
						}
					}
				}
			]
